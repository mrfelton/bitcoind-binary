{
  "name": "bitcoind-binary",
  "version": "0.0.1",
  "description": "Download the latest bitcoind binary",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=4.0.0"
  },
  "scripts": {
    "clean": "rm -r ./vendor",
    "install": "node ./scripts/prebuild-install.js"
  },
  "files": [
    "scripts",
    "bin",
    "manifest"
  ],
  "bin": {
    "bitcoind": "./bin/bitcoind"
  },
  "pre-push": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrfelton/bitcoind-binary.git"
  },
  "dependencies": {
    "hasha": "5.0.0",
    "npmlog": "4.1.2",
    "prebuild-install": "5.3.0"
  },
  "keywords": [
    "bitcoin",
    "bitcoind"
  ],
  "author": "Tom Kirkpatrick <tkp@kirkdesigns.co.uk> (http://www.kirkdesigns.co.uk/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrfelton/bitcoind-binary/issues"
  },
  "homepage": "https://github.com/mrfelton/bitcoind-binary"
}
